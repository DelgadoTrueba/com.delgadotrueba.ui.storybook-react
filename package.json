{
  "name": "@delgadotrueba/prime-react-lib",
  "version": "0.0.7",
  "description": "Description",
  "author": "delgadotrueba",
  "keywords": [
    "http",
    "httpMock",
    "httpMockService",
    "httpService"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/delgadotrueba/prime-react-lib"
  },
  "scripts": {
    "build": "npx webpack --config webpack.config.js --mode production",
    "build:dev": "npx webpack --config webpack.config.js --mode development",
    "lint": "npx eslint  -c .eslintrc.json src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "npx jest -c jest.config.js --verbose",
    "test:coverage": "npm run test -- --coverage",
    "prepare": "husky",
    "prepublishOnly": "npm run test && npm run build"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/plugin-transform-runtime": "7.25.9",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-react": "7.25.9",
    "@babel/preset-typescript": "7.26.0",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@types/jest": "29.5.12",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "babel-loader": "9.2.1",
    "commitlint": "19.3.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.2.1",
    "husky": "9.1.4",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-environment-node": "29.7.0",
    "lint-staged": "15.2.8",
    "prettier": "3.3.3",
    "ts-jest": "29.2.4",
    "ts-loader": "9.5.1",
    "typescript": "5.5.4",
    "webpack": "5.96.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.1.0"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tslib": "2.6.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.js",
        "types": "./dist/src/index.d.ts"
      }
    },
    "./public-api": {
      "import": {
        "default": "./dist/public-api.js",
        "types": "./dist/src/public-api.d.ts"
      }
    }
  },
  "sideEffects": false
}